import java.util.Scanner;
class Array
{
	public static void main(String[] args) 
	{
		int[]a={10,20,30,40,50,60,70};
		//int[]b=a;
		for(int i=0; i<a.length;i++)
		{
			for(int j=0; j<a.length;j++)
			{
				if(a[i]+a[j]==7)
			{
					System.out.println(a[i]+","+a[j]);
			}			
		}
		}
	}
}



